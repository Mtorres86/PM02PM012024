<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PM02PM012024.PageInit"
             Title="PageInit">
    <VerticalStackLayout>



        <Image x:Name="foto" BackgroundColor="AliceBlue" HeightRequest="300" WidthRequest="300"
               HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>

        <Button Text="selecciona imagen" x:Name="btn_boton" Clicked="btn_boton_Clicked"/>
        <Entry x:Name="nombres" Placeholder="Name" Keyboard="Text"  />
        <Entry x:Name="apellidos" Placeholder="Lastname" Keyboard="Text"  />
        <DatePicker x:Name="FechaNac" Format="d"/>
        <Entry x:Name="Telefono" Placeholder="Telephone Number" Keyboard="Telephone"  />

        <Button x:Name = "btn_procesar" Text="Send" Clicked="btn_procesar_Clicked"/>

            

            

    </VerticalStackLayout>
</ContentPage>
